{
  "name": "neuroagent-backend-ts",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev -p 8079",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start -p 8079",
    "start:prod": "NODE_ENV=production next start -p 8079",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css,md}\" \"tests/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,css,md}\" \"tests/**/*.{ts,tsx}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "test:compatibility": "vitest --run tests/api/compatibility.test.ts",
    "test:ci": "vitest --run --coverage --reporter=verbose",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:status": "prisma migrate status",
    "db:studio": "prisma studio",
    "db:seed": "prisma db seed",
    "validate": "npm run lint && npm run format:check && npm run type-check",
    "validate:fix": "npm run lint:fix && npm run format && npm run type-check",
    "precommit": "npm run validate:fix && npm test",
    "prebuild": "npm run db:generate",
    "postinstall": "prisma generate",
    "clean": "rm -rf .next node_modules/.cache",
    "clean:all": "rm -rf .next node_modules dist coverage",
    "docker:build": "docker build -t neuroagent-backend-ts:latest .",
    "docker:run": "docker run -p 8079:8079 --env-file .env neuroagent-backend-ts:latest",
    "docker:compose:up": "docker-compose up backend-ts",
    "docker:compose:down": "docker-compose down",
    "docker:compose:logs": "docker-compose logs -f backend-ts"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.0.0",
    "@aws-sdk/client-s3": "^3.700.0",
    "@aws-sdk/s3-request-presigner": "^3.700.0",
    "@modelcontextprotocol/sdk": "^1.25.2",
    "@openrouter/ai-sdk-provider": "^0.0.5",
    "@prisma/client": "^6.2.0",
    "ai": "^4.0.0",
    "ioredis": "^5.4.1",
    "jose": "^5.9.6",
    "next": "^15.1.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^3.24.1",
    "zod-to-json-schema": "^3.25.1"
  },
  "devDependencies": {
    "@fast-check/vitest": "^0.1.3",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.17.0",
    "eslint-config-next": "^15.1.3",
    "prettier": "^3.4.2",
    "prisma": "^6.2.0",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
